#!/usr/bin/env bash
# Stops at most 2 4-to_infinity_and_beyond processes without kill or killall
pids=$(pgrep -x "4-to_infinity_and_beyond")
self_pid=$$
count=0
for pid in $pids; do
if [ "$pid" != "$self_pid" ]; then
echo 15 > "/proc/$pid/kill"
count=$((count + 1))
if [ "$count" -eq 2 ]; then
break
fi
fi
done
